@model  List<demo.Models.Service>
@{
    ViewBag.Title = "Service";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="page-wrapper">
	<div class="container-fluid">
		<!-- Page Heading -->
		<div class="row">
			<div class="col-lg-6">

				<a href="/Service/Create" class="btn btn-primary btn-lg active "><i class="fa fa-plus" aria-hidden="true"></i> New  Service</a>

			</div>
		</div>
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">
					Service
				</h1>
				<ol class="breadcrumb">
					<li>
						<i class="fa fa-dashboard"></i>  <a href="index.html">Dashboard</a>
					</li>

					<li class="active">
						<i class="fa fa-table"></i> Service
					</li>
				</ol>
			</div>
		</div>
		<!-- /.row -->
		<div class="row">
			<div class="col-lg-6">
				<h2>Service Table</h2>
				<div class="table-responsive">
					<table class="table table-bordered table-hover">
						<thead>
							<tr>
								<th>Service ID</th>
								<th>Service Name</th>
								<th>Gender</th>
								<th>Age[From]</th>
								<th>Age[To]</th>
								<th>Service Charge</th>
								<th></th>
							</tr>
						</thead>
						<tbody>
							@for(int i=0; i<Model.Count();i++)
							{
								<tr>
									<td>
										@Model[i].id
									</td>
									<td>
										@Model[i].Service_Name
									</td>
									<td>
										@Model[i].GenderGender
									</td>
									<td>
										@Model[i].Age_From
									</td>
									<td>
										@Model[i].Age_To
									</td>
									<td>
										@Model[i].Service_Charge
									</td>
									<td>
										<a href="/Service/Create/@Model[i].id"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>
									
									</td>
								</tr>
							}
						
						</tbody>
					</table>
				</div>
			</div>

		</div>
		<!-- /.row -->
		<!-- /.row -->
	</div>
	<!-- /.container-fluid -->
</div>

