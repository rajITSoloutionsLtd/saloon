@model demo.Models.modelservicetype
@using demo.Models


@{
	ViewBag.Title = "ServiceType Create";
	Layout = "~/Views/Shared/_Layout.cshtml";
}




<h1> Create ServiceType</h1>

@using (Html.BeginForm("CreateServiceType", "ServiceType"))
{
	@Html.AntiForgeryToken()
<div id="page-wrapper">
	<div class="container-fluid">
		<!-- Page Heading -->
		<div class="row">
			<div class="col-md-10">				
						<div class="form-group">
							<label for="exampleInputEmail1">Name</label>
							<input type="text" name="Name" value="@Model.Name" class="form-control" id="exampleInputEmail1" placeholder="Service Name">
						</div>
				</div>
			@Html.HiddenFor(model => model.id)
		

				<div class="form-group">
					<div class="col-md-10">
						<input type="submit" value="Submit" class="btn btn-default" />
					</div>
				</div>

			</div>
	</div>

</div>

}

