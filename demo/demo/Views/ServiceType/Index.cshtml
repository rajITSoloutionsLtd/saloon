@using demo.Models;
@model List<modelservicetype>
@{
    ViewBag.Title = "ServiceType";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="page-wrapper">
	<div class="container-fluid">
		<!-- Page Heading -->
		<div class="row">
			<div class="col-lg-6">

				<a href="/ServiceType/Create" class="btn btn-primary btn-lg active">New ServiceType</a>

			</div>
		</div>
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">
					Service Type
				</h1>
				<ol class="breadcrumb">
					<li>
						<i class="fa fa-dashboard"></i>  <a href="index.html">Dashboard</a>
					</li>

					<li class="active">
						<i class="fa fa-table"></i> ServiceType
					</li>
				</ol>
			</div>
		</div>
		<!-- /.row -->
		<div class="row">
			<div class="col-lg-6">
				<h2>ServiceType Table</h2>
				<div class="table-responsive">
					<table class="table table-bordered table-hover">
						<thead>
							<tr>
								<th>Service id</th>
								<th>Name</th>
								<th></th>
							</tr>
						</thead>
						<tbody>
						@for (int i = 0; i < Model.Count(); i++)
						{
							<tr>
									<td>
										@Model[i].id
									 </td>
								<td>
										@Model[i].Name
								 </td>
								<td>
									<a href="/ServiceType/Create/@Model[i].id"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>

								</td>
							  </tr>
						}

					</table>
				</div>
			</div>

		</div>
		<!-- /.row -->
		<!-- /.row -->
	</div>
	<!-- /.container-fluid -->
</div>

