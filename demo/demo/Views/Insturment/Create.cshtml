
@{
    ViewBag.Title = "Instrument Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1>Instrument Dettails! </h1>
<div id="page-wrapper">
	<div class="container-fluid">
		<!-- Page Heading -->
		<div class="row">
			<div class="col-lg-12">
				<form>
					<div class="form-group">
						<label for="exampleInputEmail1">Instrument Name</label>
						<input type="text" class="form-control" id="exampleInputEmail1" placeholder="Different type of Instrument">
					</div>

					<div class="form-group">
						<label for="exampleInputEmail1">Instrument Service</label>
						<input type="text" class="form-control" id="exampleInputEmail1" placeholder="Intrument Service">
					</div>
					<div class="form-group">
						<label for="exampleInputPassword1">Instrument Number</label>
						<input type="password" class="form-control" id="exampleInputPassword1" placeholder="Number">
					</div>

					<div class="form-group">
						<label for="exampleInputEmail1">Status of Instrument</label>
						<input type="text" class="form-control" id="exampleInputEmail1" placeholder="New Instrument">
					</div>

					<div class="form-group">
						<label for="exampleInputEmail1">Instrument Assignee</label>
						<input type="text" class="form-control" id="exampleInputEmail1" placeholder="To whom instrument is being used">
					</div>


					<button type="submit" class="btn btn-default">Submit</button>
				</form>
			</div>
		</div>

	</div>

</div>

