@model List<demo.Models.ViewModel>

	<div>

		<div class="form-group">
			<label>Select-Agent</label>
			@foreach (var mdl in Model)
			{
				<label class="Select-Agent">
					<input name="AgentID[]" value="@mdl.Agentid" type="checkbox">@mdl.Agent_Name
				</label>
			}

			
		</div>

		<input type="hidden" name="Service_Charge" value="@((Model!=null && Model.Count>0 )? Model[0].Service_Charge : 0)" />


		<div class="form-group">
			<label for="exampleInputEmail1">Service Charge</label>
			<input type="text" class="form-control disabled" disabled name="Service_Charge" value="@((Model!=null && Model.Count>0 )? Model[0].Service_Charge : 0)"  id="exampleInputEmail1" placeholder="Charge">
		</div>




		<input type="hidden" class="form-control disabled"  id="exampleInputEmail1" name="NetTotal" value="@((Model!=null && Model.Count>0 )? Model[0].Net_Total : 0)" placeholder="Total Charge">




		<div class="form-group">
			<label for="exampleInputEmail1"> Net total </label>
			<input type="text" class="form-control disabled" disabled  id="exampleInputEmail1" name="NetTotal" value="@((Model!=null && Model.Count>0 )? Model[0].Net_Total : 0)"  placeholder="Total Charge">
		</div>

	</div>



